    def set0(self):
        text = self.label.text()
        text = text+"0"
        self.label.setText(text)

    def plus(self):
        text = self.label.text()
        text = text+"+"
        self.label.setText(text)

    def minus(self):
        text = self.label.text()
        text = text+"-"
        self.label.setText(text)

    def dilenya(self):
        text = self.label.text()
        text = text+"/"
        self.label.setText(text)

    def mult(self):
        text = self.label.text()
        text = text+"*"
        self.label.setText(text)

    def del_1(self):
        text = self.label.text()
        text = text[:-1]
        self.label.setText(text)

    def clear(self):
        self.label.setText("")

    def korin(self):
        text = self.label.text()
        try:
            if "^" in text:
                text = text.replace("^", "**")
            res = eval(text)
            res = math.sqrt(res)
            self.label.setText(str(res))
        except:
            self.label.setText("Wrong korin")

    def stepin(self):
        text = self.label.text()
        text = text+"**"
        self.label.setText(text)    

    def result(self):
        text = self.label.text()
        try:
            if "^" in text:
                text = text.replace("^", "**")
            res = eval(text)
            self.label.setText(str(res))
        except:
            self.label.setText("Wrong input")

    

