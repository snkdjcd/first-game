    def set1(self):
        text = self.label.text()
        text = text+"1"
        self.label.setText(text)

    def set2(self):
        text = self.label.text()
        text = text+"2"
        self.label.setText(text)

    def set3(self):
        text = self.label.text()
        text = text+"3"
        self.label.setText(text)

